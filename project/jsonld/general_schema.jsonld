      "slot_uri": "https://cellular-semantics.sanger.ac.uk/ontology/CAS/annotation_method",
      "owner": "Labelset",
      "domain_of": [
        "Labelset"
      ],
      "range": "annotation_method_options",
      "@type": "SlotDefinition"
    },
